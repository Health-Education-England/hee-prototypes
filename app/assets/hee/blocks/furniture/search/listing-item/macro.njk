{% macro searchListingItem(params) %}
<div class="hee-search-listing-item">
  <h3><a href="{{ params.href }}" title="{{ params.title }}">{{ params.title }}</a></h3>
  <span class="hee-search-listing-item__subtitle">{{ params.subtitle }}</span>
  <div class="hee-search-listing-item__summary">
    {{ params.summary }}
  </div>
  {% if params.attributes %}
    <div class="hee-search-listing-item__attributes">
      {% for attr in params.attributes %}
        <div class="hee-search-listing-item__attributes__item">
          <span class="hee-search-listing-item__attributes__label">
            {{ attr.label }}
          </span>
          <span class="hee-search-listing-item__attributes__value">
            {{ attr.value }}
          </span>
        </div>
      {% endfor %}
    </div>
  {% endif %}
  <div class="hee-search-listing-item__url">
    <a href="{{ params.href }}">{{ params.url }}</a>
  </div>
  <span class="hee-search-listing-item__updated">{{ params.lastUpdated }}</span>
</div>
{% endmacro %}
