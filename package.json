{
  "name": "HEE-prototypes",
  "version": "2.0.0",
  "description": "Rapidly create HTML prototypes of NHS websites and services.",
  "main": "app.js",
  "scripts": {
    "serve": "gulp serve",
    "build": "gulp build",
    "build:prod": "gulp build --prod",
    "test": "npm run backstop:test",
    "watch": "gulp watch",
    "backstop:ref": "npm run backstop:generate-config && start-server-and-test serve http://localhost:3000 'npx backstop --config=./tests/backstop/config/backstop.json reference --docker'",
    "backstop:test": "npm run backstop:generate-config && start-server-and-test serve http://localhost:3000 'npx backstop --config=./tests/backstop/config/backstop.json test --docker'",
    "backstop:approve": "npx backstop --config=./tests/backstop/config/backstop.json approve",
    "backstop:generate-config": "node tests/backstop/generateConfig.js"
  },
  "author": "https://github.com/nhsuk/",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.5",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.0",
    "browser-sync": "^2.27.9",
    "client-sessions": "^0.8.0",
    "dotenv": "^6.0.0",
    "express": "^4.18.0",
    "express-session": "^1.17.1",
    "glob": "^10.2.6",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-html-beautify": "^1.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-nunjucks": "^5.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "keypather": "^3.0.0",
    "nhsuk-frontend": "4.1.0",
    "node-sass": "^7.0",
    "nunjucks": "^3.2.2",
    "path": "^0.12.7",
    "start-server-and-test": "^2.0.0",
    "webpack": "^5.72.0",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "backstopjs": "^6.2.1"
  }
}
