{
  "id": "blocks",
  "pathPattern": "./app/views/blocks/**/examples/*.html",
  "overrides": [
    {
      "label": "blocks-header-navigation-desktop-submenu-click",
      "clickSelectors": [
        "li.nhsuk-subnav"
      ],
      "url": "http://localhost:3000/blocks/scaffolding/examples/header-navigation.html",
      "viewports": [
        {
          "label": "desktop",
          "width": 1280,
          "height": 720
        }
      ]
    },
    {
      "label": "blocks-header-navigation-mobile-tablet-submenu-click",
      "clickSelectors": [
        "button.nhsuk-header__menu-toggle",
        "li.nhsuk-subnav"
      ],
      "url": "http://localhost:3000/blocks/scaffolding/examples/header-navigation.html",
      "viewports": [
        {
          "label": "tablet",
          "width": 768,
          "height": 1024
        },
        {
          "label": "mobile",
          "width": 375,
          "height": 667
        }
      ]
    },
    {
      "label": "blocks-main-media-embed",
      "clickSelectors": [
        ".nhsuk-media__transcript a"
      ],
      "url": "http://localhost:3000/blocks/content/examples/main-media-embed.html"
    },
    {
      "label": "blocks-main-table-expander-open",
      "clickSelectors": [
        ".hee-table-expander__toggle a"
      ],
      "url": "http://localhost:3000/blocks/content/examples/main-table-expander.html"
    }
  ]
}
