{
  "name": "hee-prototypes",
  "version": "2.0.0",
  "description": "Rapidly create HTML prototypes of NHS websites and services.",
  "main": "app.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "serve": "gulp serve",
    "build": "gulp build",
    "build:prod": "gulp build --prod",
    "watch": "gulp watch",
    "test": "npm run backstop:test",
    "backstop:ref": "docker-compose run backstopjs npm run backstop:ref",
    "backstop:test": "docker-compose run backstopjs npm run backstop:test",
    "backstop:approve": "docker-compose run backstopjs npm run backstop:approve",
    "backstop:generate-config": "docker-compose run backstopjs npm run backstop:generate-config",
    "playwright:test": "docker-compose run playwright npx playwright test --config ./tests/playwright/config/playwright.config.js",
    "playwright:test-trace": "docker-compose run playwright npx playwright test --config ./tests/playwright/config/playwright.config.js  --trace on",
    "playwright:report": "gulp playwright-report",
    "prettier:check": "prettier app/assets/hee/**/*.scss app/assets/hee/**/*.js --check",
    "prettier:fix": "prettier app/assets/hee/**/*.scss app/assets/hee/**/*.js --write",
    "lint:css": "npx stylelint -f verbose --config ./tests/linters/.stylelintrc.js app/**/*.scss",
    "lint:css:fix": "npx stylelint --fix -f verbose --config ./tests/linters/.stylelintrc.js app/**/*.scss",
    "lint:js": "npx eslint -c ./tests/linters/.eslintrc.js app/assets/hee/**/*.js",
    "lint:js:fix": "npx eslint --fix -c ./tests/linters/.eslintrc.js app/assets/hee/**/*.js"
  },
  "author": "https://github.com/Health-Education-England/",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.7",
    "@playwright/test": "1.36.1",
    "babel-loader": "^8.2.5",
    "dotenv": "^6.0.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-beautify": "^3.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-minify": "^3.1.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-nunjucks": "^5.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nhsuk-frontend": "^7.0.0",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.27",
    "prettier": "3.0.0",
    "sass": "^1.63.5",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "webpack": "^5.87.0",
    "webpack-stream": "^7.0.0"
  },
  "resolutions": {
    "ansi-regex": "^4.1.1",
    "decode-uri-component": "^0.2.1",
    "http-cache-semantics": "^4.1.1",
    "glob-parent": "^6.0.1",
    "got": "^11.8.5",
    "json5": "^2.2.2",
    "minimist": "^1.2.6",
    "semver": "^7.5.2",
    "terser": "^4.8.1"
  },
  "lint-staged": {
    "*.scss": [
      "npx prettier --write",
      "npx stylelint --fix -f verbose --config ./tests/linters/.stylelintrc.js"
    ],
    "*.js": "npx eslint --fix -c ./tests/linters/.eslintrc.js"
  }
}
