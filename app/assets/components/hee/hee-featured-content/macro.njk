{%- from 'components/hee/hee-listing-item/macro.njk' import listingItem %}

{% macro featuredContent(items) %}
  <div class="hee-featured-content">
    {% for item in items %}
      <div class="hee-featured-content__item">
        {{ listingItem(item) }}
      </div>
    {% endfor %}
  </div>
{% endmacro %}
